{% if content %}
	<section {{ attributes.addClass('spaker_mod_portfolio') }}>
		<div class="container">
			<!-- Tabs navs -->
			<div class="row aos-init aos-animate" data-aos="fade-up" data-aos-delay="200">
				<div class="col-lg-12 d-flex justify-content-center">
					{#<ul class="portfolio-flters">
						<li data-filter="*" class="filter-active">
							{{ content.first_list }}
						</li>
						<li data-filter=".filter-app">
							{{ content.second_list }}
						</li>
						<li data-filter=".filter-card">
							{{ content.third_list }}
						</li>
						<li data-filter=".filter-web">
							{{ content.fourth_list }}
						</li>
					</ul>#}
					<!-- Tabs navs -->
					<ul class="nav nav-tabs mb-3 portfolio-flters" id="ex1" role="tablist">
						<li class="nav-item" role="presentation">
							<a
							class="nav-link active"
							id="ex1-tab-1"
							data-mdb-toggle="tab"
							href="#ex1-tabs-1"
							role="tab"
							aria-controls="ex1-tabs-1"
							aria-selected="true"
							>
								{% if content.first_list %}
									{{ content.first_list }}
								{% endif %}
							</a
							>
						</li>
						<li class="nav-item" role="presentation">
							<a
							class="nav-link"
							id="ex1-tab-2"
							data-mdb-toggle="tab"
							href="#ex1-tabs-2"
							role="tab"
							aria-controls="ex1-tabs-2"
							aria-selected="false"
							>	
								{% if content.second_list %}
									{{ content.second_list }}
								{% endif %}
							</a
							>
						</li>
						<li class="nav-item" role="presentation">
							<a
							class="nav-link"
							id="ex1-tab-3"
							data-mdb-toggle="tab"
							href="#ex1-tabs-3"
							role="tab"
							aria-controls="ex1-tabs-3"
							aria-selected="false"
							>
								{% if content.third_list %}
									{{ content.third_list }}
								{% endif %}
							</a
							>
						</li>
						<li class="nav-item" role="presentation">
							<a
							class="nav-link"
							id="ex1-tab-4"
							data-mdb-toggle="tab"
							href="#ex1-tabs-4"
							role="tab"
							aria-controls="ex1-tabs-4"
							aria-selected="false"
							>
								{% if content.fourth_list %}
									{{ content.fourth_list }}
								{% endif %}
							</a
							>
						</li>
					</ul>
					<!-- Tabs navs -->
					<!-- Tabs content -->
					<!-- /.navbar-collapse -->
					<div class="row portfolio-container aos-init aos-animate" data-aos="fade-up" data-aos-delay="400" style="position: relative; height: 891px;">
						<div class="tab-content" id="ex1-content">
							<div
								class="tab-pane fade show active"
								id="ex1-tabs-1"
								role="tabpanel"
								aria-labelledby="ex1-tab-1"
							>
								{% if content.teaser_container_one %}
									{% for item in content.teaser_container_one|array_elements(region_attriutes.teaser_container_one.addClass('')) %}
										{{ item }}
									{% endfor %}
								{% endif %}
							</div>
							<div class="tab-pane fade" id="ex1-tabs-2" role="tabpanel" aria-labelledby="ex1-tab-2">
								{% if content.teaser_container_two %}
									{% for item in content.teaser_container_two|array_elements(region_attriutes.teaser_container_two.addClass('')) %}
										{{ item }}
									{% endfor %}
								{% endif %}
							</div>
							<div class="tab-pane fade" id="ex1-tabs-3" role="tabpanel" aria-labelledby="ex1-tab-3">
								{% if content.teaser_container_three %}
									{% for item in content.teaser_container_three|array_elements(region_attriutes.teaser_container_three.addClass('')) %}
										{{ item }}
									{% endfor %}
								{% endif %}
							</div>
							<div class="tab-pane fade" id="ex1-tabs-4" role="tabpanel" aria-labelledby="ex1-tab-4s">
								{% if content.teaser_container_four %}
									{% for item in content.teaser_container_four|array_elements(region_attriutes.teaser_container_four.addClass('')) %}
										{{ item }}
									{% endfor %}
								{% endif %}
							</div>
						</div>
						{#% if content.teaser_container_one %}
							{% for item in content.teaser_container_one|array_elements(region_attriutes.teaser_container_one.addClass('')) %}
								{{ item }}
							{% endfor %}
						{% endif %#}
					</div>
					<!-- Tabs content -->
				</div>
			</div>
			
		</div>
	</section>
{% endif %}