{% if content %}
	<section {{ attributes.addClass('spaker_mod_contact') }}>
		<div class="container">
			<div class="row">
				<div class=" col-lg-12 section-vs-title  aos-init" data-aos="fade-up">
					{% if content.title %}
					<h2 {{ region_attributes.title.addClass('h1')}}>
						{{ content.title }}
					</h2>
					{% endif %}
				</div>
			</div>
			<div class="row">
				<div class="col-lg-4 col-md-6 aos-init" data-aos="fade-up" data-aos-delay="100">
					<div class="contact-about">
						{% if content.sub_title %}
						<h3 {{ region_attributes.content.sub_title.addClass('h3')}}>
							{{ content.sub_title }}
						</h3>
						{% endif %}
						{% if content.text_one %}
						<div {{ region_attributes.content.text_one.addClass('')}} >
							{{ content.text_one }}
						</div>
						{% endif %}
						{% if content.social|has_value %}
							{{ content.social }}
						{% endif %}
					</div>
				</div>
				<div class="col-lg-3 col-md-6 mt-4 mt-md-0 aos-init" data-aos="fade-up" data-aos-delay="200">
					<div class="info">
						<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" width="30" height="30" preserveaspectratio="xMidYMid meet" viewbox="0 0 24 24"><path d="M12 20.9l4.95-4.95a7 7 0 1 0-9.9 0L12 20.9zm0 2.828l-6.364-6.364a9 9 0 1 1 12.728 0L12 23.728zM12 13a2 2 0 1 0 0-4a2 2 0 0 0 0 4zm0 2a4 4 0 1 1 0-8a4 4 0 0 1 0 8z" fill="#8bc4ea"/>
						</svg>
						{% if content.text_two %}
						<div {{ region_attributs.text_two.addClass('info_text')}}>
							{{ content.text_two }}
						</div>
						{% endif %}
					</div>
					<div class="info">
						<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" width="30" height="30" preserveaspectratio="xMidYMid meet" viewbox="0 0 24 24"><path d="M22 20.007a1 1 0 0 1-.992.993H2.992A.993.993 0 0 1 2 20.007V19h18V7.3l-8 7.2l-10-9V4a1 1 0 0 1 1-1h18a1 1 0 0 1 1 1v16.007zM4.434 5L12 11.81L19.566 5H4.434zM0 15h8v2H0v-2zm0-5h5v2H0v-2z" fill="#8bc4ea"/>
						</svg>
						{% if content.text_three %}
						<div {{ region_attributs.text_three.addClass('info_text')}}>
							{{ content.text_three }}
						</div>
						{% endif %}
					</div>
					<div class="info">
						<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" role="img" width="30" height="30" preserveaspectratio="xMidYMid meet" viewbox="0 0 24 24"><path d="M9.366 10.682a10.556 10.556 0 0 0 3.952 3.952l.884-1.238a1 1 0 0 1 1.294-.296a11.422 11.422 0 0 0 4.583 1.364a1 1 0 0 1 .921.997v4.462a1 1 0 0 1-.898.995A15.51 15.51 0 0 1 18.5 21C9.94 21 3 14.06 3 5.5c0-.538.027-1.072.082-1.602A1 1 0 0 1 4.077 3h4.462a1 1 0 0 1 .997.921A11.422 11.422 0 0 0 10.9 8.504a1 1 0 0 1-.296 1.294l-1.238.884zm-2.522-.657l1.9-1.357A13.41 13.41 0 0 1 7.647 5H5.01c-.006.166-.009.333-.009.5C5 12.956 11.044 19 18.5 19c.167 0 .334-.003.5-.01v-2.637a13.41 13.41 0 0 1-3.668-1.097l-1.357 1.9a12.442 12.442 0 0 1-1.588-.75l-.058-.033a12.556 12.556 0 0 1-4.702-4.702l-.033-.058a12.442 12.442 0 0 1-.75-1.588z" fill="#8bc4ea"/>
						</svg>
						{% if content.text_four %}
						<div {{ region_attributs.text_four.addClass('info_text')}}>
							{{ content.text_four }}
						</div>
						{% endif %}
					</div>
				</div>
				<div class="col-lg-5 col-md-12 aos-init" data-aos="fade-up" data-aos-delay="300">
					{% if content.contact_form %}
					<div {{ region_attributes.contact_form.addClass('')}}>
						{{ content.contact_form }}
					</div>
					{% endif %}
				</div>
			</div>
		</div>
	</section>
{% endif %}
